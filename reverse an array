#include <iostream>
using namespace std;

void reverse(int arr[], int start, int end){
  if (start>=end){
    return;
  }
  int c = arr[start];
  arr[start] = arr[end];
  arr[end] = c;

  reverse(arr, start+1, end-1);
  return;
}

int main(){
  int n;
  cin >> n;

  int array[n];
  for (int i = 0; i < n; i++){
    cin >> array[i];
  }
  
  reverse(array, 0, n-1);
  
  for (int i = 0; i < n; i++){
    cout<< array[i]<<" ";
  }
}

